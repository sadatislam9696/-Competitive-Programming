#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,x,m;cin>>n;
    int a[n],cnt=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>x;
    bool found = false;
    for(int i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            m=i;
            found = true;
            break;
        }
    }
    if(found==false)
    {
        cout<<-1<<'\n';
    }
    else
        cout<<m<<'\n';

    return 0;
}





