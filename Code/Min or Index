#include<bits/stdc++.h>
using namespace std;
int main()
{
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++)
   {
       cin>>a[i];
   }
   int mi=10e9;
   
   for(int i=0;i<n;i++)
   {
       mi=min(mi,a[i]);
   }
   cout<<mi<<' ';
   for(int i=0;i<n;i++)
   {
       if(a[i]==mi)
       {
           cout<<mi<<'\n';
           break;
       }
   }
    return 0;
}





