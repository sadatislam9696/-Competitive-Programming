#include<bits/stdc++.h>
using namespace std;
int main()
{
    int num1,num2,num3;
    cout<<"Enter 3 number : ";
    cin>>num1>>num2>>num3;

    if (num1>num2&&num1>num3)
    {
        cout<<"Large number is : "<<num1;
    }

    else if(num2>num1&&num2>num3)
    {
        cout<<"Large number is : "<<num2;
    }

    else
    {
        cout<<"Large number is : "<<num3;
    }

}
